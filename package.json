{
  "name": "rlscripting",
  "publisher": "haiku",
  "displayName": "RLScripting",
  "description": "Gives you syntax highlighting when modding RealLive scripts in VSC",
  "version": "0.0.1",
  "icon": "./ext_icon.png",
  "repository": {
    "url": "https://github.com/haiku-balls/rlscripting-vscode"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Themes"
  ],
  "contributes": {
    "languages": [{
      "id": "rl",
      "aliases": ["RealLive Scripts", "rl"],
      "extensions": [".org",".utf"],
      "configuration": "./language-configuration.json"
    }],
    "snippets": [
      {
        "language": "rl",
        "path": "./snippets.json"
      }
    ],
    "grammars": [{
      "language": "rl",
      "scopeName": "source.rl",
      "path": "./syntaxes/rl.tmLanguage.json"
    }]
  }

}
